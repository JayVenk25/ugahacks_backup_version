{
  "expo": {
    "name": "Cauley Creek Tracker",
    "slug": "cauley-creek-tracker",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2E7D32"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cauleycreek.tracker",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs location access to track parking availability at Cauley Creek Park.",
        "NSLocationAlwaysUsageDescription": "This app needs location access to track parking availability at Cauley Creek Park."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2E7D32"
      },
      "package": "com.cauleycreek.tracker",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Cauley Creek Tracker to use your location to track parking availability."
        }
